        android:textSize="@dimen/_10ssp"
        android:textStyle="normal"

        tools:ignore="HardcodedText" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
