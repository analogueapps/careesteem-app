<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/medication_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp"
        android:fontFamily="@font/robotoslab_regular"
        android:gravity="start"
        android:hint="Activity"
        android:textSize="@dimen/_12ssp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Medicine Name"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvMedicineName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="#27998933"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Dose"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvDose"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="#27998933"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Route"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvRoute"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="#27998933"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Time/Frequency"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvTimeFrequency"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="#27998933"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Self-Administration"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvSelfAdministration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>

    <View
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginEnd="@dimen/_5sdp"
        android:background="#27998933"
        android:layout_width="match_parent"
        android:layout_height="1dp" />

    <androidx.appcompat.widget.LinearLayoutCompat
        android:padding="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:text="Self-administer fully or partially?"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:fontFamily="@font/robotoslab_regular"
            android:gravity="center"
            android:text=":"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvSelfAdministerFully"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:fontFamily="@font/robotoslab_regular"
            android:textSize="@dimen/_10ssp"
            android:textStyle="normal"

            tools:ignore="HardcodedText" />

    </androidx.appcompat.widget.LinearLayoutCompat>


</androidx.appcompat.widget.LinearLayoutCompat>